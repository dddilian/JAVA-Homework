import java.util.Scanner;

public class Task9 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in, "UTF-8");

		int number = input.nextInt();

		int hundreds = number / 100 % 10;
		int tens = number / 10 % 10;
		int units = number % 10;

		if (hundreds != 0) {
			switch (hundreds) {
			case 1:
				System.out.print("one hundred ");
				break;
			case 2:
				System.out.print("two hundred ");
				break;
			case 3:
				System.out.print("three hundred ");
				break;
			case 4:
				System.out.print("four hundred ");
				break;
			case 5:
				System.out.print("five hundred ");
				break;
			case 6:
				System.out.print("six hundred ");
				break;
			case 7:
				System.out.print("seven hundred ");
				break;
			case 8:
				System.out.print("eight hundred ");
				break;
			case 9:
				System.out.print("nine hundred ");
				break;
			}
		}
		if (tens == 0) {
			switch (units) {
			case 0:
				//System.out.println("zero");
				break;
			case 1:
				System.out.println("and one");
				break;
			case 2:
				System.out.println("and two");
				break;
			case 3:
				System.out.println("and three");
				break;
			case 4:
				System.out.println("and four");
				break;
			case 5:
				System.out.println("and five");
				break;
			case 6:
				System.out.println("and six");
				break;
			case 7:
				System.out.println("and seven");
				break;
			case 8:
				System.out.println("and eight");
				break;
			case 9:
				System.out.println("and nine");
				break;
			}
		}
	}
}