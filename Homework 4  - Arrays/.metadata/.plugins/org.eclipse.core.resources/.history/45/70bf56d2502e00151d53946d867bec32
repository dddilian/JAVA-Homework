import java.util.Scanner;

public class Task10ANWithoutRemainder {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in, "ÃœTF-8");

		System.out.println("Enter two numbers - \"a\" and \"b\". \"a\" should be bigger than \"b\": ");
		System.out.println("Enter a");
		int a = input.nextInt();
		System.out.println("Enter b");
		int b = input.nextInt();

		if (a < b) {
			System.out.println("Enter a is smaller");
		} else {
			System.out.println("The numbers below are divided by 3 without remainder");
			for (int i = a; i < b; i++) {
				if (i % 3 == 0) {
					System.out.println(i);
				}

			}
		}
		input.close();
	}
}