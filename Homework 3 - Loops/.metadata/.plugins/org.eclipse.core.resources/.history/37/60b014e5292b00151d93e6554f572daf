import java.util.Scanner;

public class Task2 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Enter five numbers: ");

		
		int biggest = 0;
		

		for (int i = 0; i <= 5; i++) {
			System.out.println("Enter a number: ");
			int n = input.nextInt();
			
			if (n > biggest) {
				biggest = n;
			}
			
		}
		System.out.println(biggest);
		input.close();
	}
}