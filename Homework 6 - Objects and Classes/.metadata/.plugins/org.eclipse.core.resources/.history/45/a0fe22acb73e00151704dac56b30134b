package Task2Cars;

public class Car {

	protected int price;

	public Car(int price) {
		super();
		this.price = price;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public void printPrice() {
		
		System.out.println("Car's price is: " + this.price);
	}
}