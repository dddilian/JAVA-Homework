import java.util.Random;

public class Task3 {

	public static void main(String[] args) {
		double temp = 0;
		     
		Random randomGenerator = new Random();
	
		
		for (int i = 1; i <= 3; ++i) {
			double random = Math.random();
			System.out.println(random);
			if (i == 1){
				temp=random;
			}
			temp = random * random;
		}
		System.out.println(temp);
	}
}
