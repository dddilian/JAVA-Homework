import java.util.Random;

public class Task3 {

	public static void main(String[] args) {
		long temp = 0;

		Random rand = new Random();

		for (int i = 1; i <= 3; ++i) {
			int random = rand.nextInt(1000) + 1;   // This will give value from 1 to 1000: rand.nextInt(1000) + 1;
			System.out.println(random);

			temp = random * random;
		}
		System.out.println(temp);
	}
}
